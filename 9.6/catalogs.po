#: catalogs.xml:4394(structname) catalogs.xml:4558(structname)
#: catalogs.xml:4834(structname) catalogs.xml:6320(structname)
#: catalogs.xml:6466(structname) catalogs.xml:6549(structname)
#: catalogs.xml:6646(structname) catalogs.xml:6720(structname)
#: catalogs.xml:7943(structname) catalogs.xml:8330(structname)
#: catalogs.xml:8403(structname) catalogs.xml:8995(structname)
#: catalogs.xml:9088(structname) catalogs.xml:9539(structname)
#: catalogs.xml:9720(structname) catalogs.xml:10112(structname)
msgid "pg_namespace"
msgstr "pg_namespace"

#: catalogs.xml:1581(literal) catalogs.xml:1948(literal)
#: catalogs.xml:2067(literal) catalogs.xml:2339(literal)
#: catalogs.xml:2669(literal) catalogs.xml:3213(literal)
#: catalogs.xml:4296(literal) catalogs.xml:4394(literal)
#: catalogs.xml:4558(literal) catalogs.xml:4834(literal)
