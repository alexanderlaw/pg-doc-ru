msgstr "Семейство операторов, к которому относится эта запись"

#: catalogs.xml:653(structfield)
msgid "amoplefttype"
msgstr "amoplefttype"

#: catalogs.xml:656(entry)
msgid "Left-hand input data type of operator"
msgstr "Тип данных левого операнда оператора"

#: catalogs.xml:660(structfield)
msgid "amoprighttype"
msgstr "amoprighttype"

#: catalogs.xml:663(entry)
msgid "Right-hand input data type of operator"
msgstr "Тип данных правого операнда оператора"

#: catalogs.xml:667(structfield)
msgid "amopstrategy"
msgstr "amopstrategy"

#: catalogs.xml:670(entry)
msgid "Operator strategy number"
msgstr "Номер стратегии оператора"

#: catalogs.xml:674(structfield)
msgid "amoppurpose"
msgstr "amoppurpose"

#: catalogs.xml:677(entry)
msgid ""
"Operator purpose, either <literal>s</literal> for search or <literal>o</"
"literal> for ordering"
msgstr ""
"Предназначение оператора: <literal>s</literal> — поиск (search), <literal>o</"
"literal> — упорядочивание (ordering)"

#: catalogs.xml:682(structfield)
msgid "amopopr"
msgstr "amopopr"

#: catalogs.xml:685(entry)
msgid "OID of the operator"
msgstr "OID оператора"

