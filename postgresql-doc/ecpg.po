msgid "GET DESCRIPTOR"
msgstr "GET DESCRIPTOR"

#. +> REL_10 REL9_6_3 REL9_6
#: ecpg.xml:6978(refpurpose)
msgid "get information from an SQL descriptor area"
msgstr "получить информацию из области дескриптора SQL"

#. +> REL_10 REL9_6_3 REL9_6
