msgstr "pg_user_mappings"

#. +> REL_10 REL9_6_3 REL9_6
#: catalogs.xml:11028(indexterm)
msgid "<primary>pg_user_mappings</primary>"
msgstr "<primary>pg_user_mappings</primary>"

#. +> REL_10 REL9_6_3 REL9_6
#: catalogs.xml:11032(para)
