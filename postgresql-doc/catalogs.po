msgid ""
"Type of the lockable object: <literal>relation</literal>, <literal>extend</"
"literal>, <literal>page</literal>, <literal>tuple</literal>, "
"<literal>transactionid</literal>, <literal>virtualxid</literal>, "
"<literal>object</literal>, <literal>userlock</literal>, or "
"<literal>advisory</literal>"
msgstr ""
