#: ref/create_access_method.xml:24(synopsis)
#, no-wrap
msgid ""
"CREATE ACCESS METHOD <placeholder-1/>\n"
"    TYPE <placeholder-2/>\n"
"    HANDLER <placeholder-3/>"
msgstr ""
"CREATE ACCESS METHOD <placeholder-1/>\n"
"    TYPE <placeholder-2/>\n"
